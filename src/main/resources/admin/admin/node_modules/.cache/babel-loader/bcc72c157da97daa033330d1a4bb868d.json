placeholder=\"是否通过\">\n\t\t\t\t\t\t\t<el-option v-for=\"(item,index) in sfshOptions\" v-bind:key=\"index\" :label=\"item\" :value=\"item\"></el-option>\n\t\t\t\t\t\t</el-select>\n\t\t\t\t\t</div>\n\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\"4px\",\"background\":\"rgba(154, 92, 85, .7)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' type=\"success\" @click=\"search()\">查询</el-button>\n\t\t\t\t</el-row>\n\n\t\t\t\t<el-row :style='{\"margin\":\"20px 0\",\"justifyContent\":\"flex-end\",\"display\":\"flex\"}'>\n\t\t\t\t\t<el-button :style='{\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 0\",\"outline\":\"none\",\"borderColor\":\"rgba(89, 54, 47, 1)\",\"color\":\"#fff\",\"borderRadius\":\"4px\",\"background\":\"rgba(154, 92, 85, 1)\",\"borderWidth\":\"3px 0\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('zuzhibaoming','新增')\" type=\"success\" @click=\"addOrUpdateHandler()\">新增</el-button>\n\t\t\t\t\t<el-button :style='{\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 0 0\",\"outline\":\"none\",\"borderColor\":\"rgba(89, 54, 47, 1)\",\"color\":\"#fff\",\"borderRadius\":\"4px\",\"background\":\"rgba(154, 92, 85, 1)\",\"borderWidth\":\"3px 0\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"40px\"}' v-if=\"isAuth('zuzhibaoming','删除')\" :disabled=\"dataListSelections.length <= 0\" type=\"danger\" @click=\"deleteHandler()\">删除</el-button>\n\n\n\n\n\t\t\t\t</el-row>\n\t\t\t</el-form>\r\n\t\t\t\n\t\t\t<!-- <div> -->\n\t\t\t\t<el-table class=\"tables\"\r\n\t\t\t\t\t:stripe='false'\r\n\t\t\t\t\t:style='{\"width\":\"100%\",\"padding\":\"0\",\"borderColor\":\"rgba(111, 111, 151, 1)\",\"borderStyle\":\"solid\",\"borderWidth\":\"1px 0 0 1px\",\"background\":\"#fff\"}' \r\n\t\t\t\t\tv-if=\"isAuth('zuzhibaoming','查看')\"\n\t\t\t\t\t:data=\"dataList\"\n\t\t\t\t\tv-loading=\"dataListLoading\"\n\t\t\t\t@selection-change=\"selectionChangeHandler\">\n\t\t\t\t\t<el-table-column :resizable='true' type=\"selection\" align=\"center\" width=\"50\"></el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' label=\"索引\" type=\"index\" width=\"50\" />\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"zuzhibianhao\"\n\t\t\t\t\tlabel=\"组织编号\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.zuzhibianhao}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"zuzhimingcheng\"\n\t\t\t\t\tlabel=\"组织名称\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.zuzhimingcheng}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"zhiyuanzhezhanghao\"\n\t\t\t\t\tlabel=\"志愿者账号\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.zhiyuanzhezhanghao}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"zhiyuanzhexingming\"\n\t\t\t\t\tlabel=\"志愿者姓名\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.zhiyuanzhexingming}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false'  \n\t\t\t\t\t\tprop=\"baomingshijian\"\n\t\t\t\t\tlabel=\"报名时间\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t{{scope.row.baomingshijian}}\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' prop=\"shhf\" label=\"审核回复\"></el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' prop=\"sfsh\" label=\"审核状态\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<span style=\"margin-right:10px\" v-if=\"scope.row.sfsh=='是'\">通过</span>\n\t\t\t\t\t\t\t<span style=\"margin-right:10px\" v-if=\"scope.row.sfsh=='否'\">未通过</span>\n\t\t\t\t\t\t\t<span style=\"margin-right:10px\" v-if=\"scope.row.sfsh=='待审核'\">待审核</span>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column :resizable='true' :sortable='false' v-if=\"isAuth('zuzhibaoming','审核')\" prop=\"sfsh\" label=\"审核\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<el-button  type=\"text\" size=\"small\" @click=\"shDialog(scope.row)\">审核</el-button>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t\t<el-table-column width=\"300\" label=\"操作\">\n\t\t\t\t\t\t<template slot-scope=\"scope\">\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 5px 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\" 0px 0px 20px 20px\",\"background\":\"rgba(154, 92, 85, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\" isAuth('zuzhibaoming','查看')\" type=\"success\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id,'info')\">详情</el-button>\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 5px 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\" 0px 0px 20px 20px\",\"background\":\"rgba(154, 92, 85, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\" isAuth('zuzhibaoming','修改')\" type=\"primary\" size=\"mini\" @click=\"addOrUpdateHandler(scope.row.id)\">修改</el-button>\n\n\n\n\n\n\t\t\t\t\t\t\t<el-button :style='{\"border\":\"0\",\"cursor\":\"pointer\",\"padding\":\"0 24px\",\"margin\":\"0 10px 5px 0\",\"outline\":\"none\",\"color\":\"#fff\",\"borderRadius\":\" 0px 0px 20px 20px\",\"background\":\"rgba(154, 92, 85, 1)\",\"width\":\"auto\",\"fontSize\":\"14px\",\"height\":\"32px\"}' v-if=\"isAuth('zuzhibaoming','删除') \" type=\"danger\" size=\"mini\" @click=\"deleteHandler(scope.row.id)\">删除</el-button>\n\t\t\t\t\t\t</template>\n\t\t\t\t\t</el-table-column>\n\t\t\t\t</el-table>\n\t\t\t\t<el-pagination\r\n\t\t\t\t\t@size-change=\"sizeChangeHandle\"\n\t\t\t\t\t@current-change=\"currentChangeHandle\"\n\t\t\t\t\t:current-page=\"pageIndex\"\n\t\t\t\t\tbackground\r\n\t\t\t\t\t:page-sizes=\"[10, 20, 30, 50]\"\r\n\t\t\t\t\t:page-size=\"pageSize\"\r\n\t\t\t\t\t:layout=\"layouts.join()\"\r\n\t\t\t\t\t:total=\"totalPage\"\r\n\t\t\t\t\tprev-text=\"<\"\r\n\t\t\t\t\tnext-text=\">\"\r\n\t\t\t\t\t:hide-on-single-page=\"false\"\r\n\t\t\t\t\t:style='{\"padding\":\"0\",\"margin\":\"20px auto\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"textAlign\":\"center\",\"width\":\"100%\",\"fontWeight\":\"500\"}'\n\t\t\t\t></el-pagination>\n\t\t\t<!-- </div> -->\n\t\t</template>\r\n\t\t\n\t\t<!-- 添加/修改页面  将父组件的search方法传递给子组件-->\n\t\t<add-or-update v-if=\"addOrUpdateFlag\" :parent=\"this\" ref=\"addOrUpdate\"></add-or-update>\n\n\n\t\t<el-dialog title=\"审核\" :visible.sync=\"sfshVisiable\" width=\"50%\">\n\t\t\t<el-form ref=\"form\" :model=\"form\" label-width=\"80px\">\n\t\t\t\t<el-form-item label=\"审核状态\">\n\t\t\t\t\t<el-select v-model=\"shForm.sfsh\" placeholder=\"审核状态\">\n\t\t\t\t\t\t<el-option label=\"通过\" value=\"是\"></el-option>\n\t\t\t\t\t\t<el-option label=\"不通过\" value=\"否\"></el-option>\n\t\t\t\t\t\t<el-option label=\"待审核\" value=\"待审核\"></el-option>\n\t\t\t\t\t</el-select>\n\t\t\t\t</el-form-item>\n\t\t\t\t<el-form-item label=\"内容\">\n\t\t\t\t\t<el-input type=\"textarea\" :rows=\"8\" v-model=\"shForm.shhf\"></el-input>\n\t\t\t\t</el-form-item>\n\t\t\t</el-form>\n\t\t\t<span slot=\"footer\" class=\"dialog-footer\">\n\t\t\t\t<el-button @click=\"shDialog\">取 消</el-button>\n\t\t\t\t<el-button type=\"primary\" @click=\"shHandler\">确 定</el-button>\n\t\t\t</span>\n\t\t</el-dialog>\n\n\n\n\t</div>\n</template>\r\n\n<script>\nimport axios from 'axios'\nimport AddOrUpdate from \"./add-or-update\";\nexport default {\n  data() {\n    return {\r\n      searchForm: {\n        key: \"\"\n      },\n      form:{},\n      dataList: [],\n      pageIndex: 1,\n      pageSize: 10,\n      totalPage: 0,\n      dataListLoading: false,\n      dataListSelections: [],\n      showFlag: true,\n      sfshVisiable: false,\n      shForm: {},\n      chartVisiable: false,\n      chartVisiable1: false,\n      chartVisiable2: false,\n      chartVisiable3: false,\n      chartVisiable4: false,\n      chartVisiable5: false,\n      addOrUpdateFlag:false,\n      layouts: [\"total\",\"prev\",\"pager\",\"next\",\"sizes\"],\n\n    };\n  },\n  created() {\n    this.init();\n    this.getDataList();\n    this.contentStyleChange()\r\n  },\n  mounted() {\n  },\n  filters: {\n    htmlfilter: function (val) {\n      return val.replace(/<[^>]*>/g).replace(/undefined/g,'');\n    }\n  },\n  components: {\n    AddOrUpdate,\n  },\n  methods: {\n\n    contentStyleChange() {\n      this.contentPageStyleChange()\n    },\n    // 分页\n    contentPageStyleChange(){\n      let arr = []\n\n      // if(this.contents.pageTotal) arr.push('total')\n      // if(this.contents.pageSizes) arr.push('sizes')\n      // if(this.contents.pagePrevNext){\n      //   arr.push('prev')\n      //   if(this.contents.pagePager) arr.push('pager')\n      //   arr.push('next')\n      // }\n      // if(this.contents.pageJumper) arr.push('jumper')\n      // this.layouts = arr.join()\n      // this.contents.pageEachNum = 10\n    },\n\n\n\n\n\n\n\n\n    init () {\n        this.sfshOptions = \"是,否,待审核\".split(',');\n    },\n    search() {\n      this.pageIndex = 1;\n      this.getDataList();\n    },\n\n    // 获取数据列表\n    getDataList() {\n      this.dataListLoading = true;\n      let params = {\n        page: this.pageIndex,\n        limit: this.pageSize,\n        sort: 'id',\n        order: 'desc',\n      }\n          if(this.searchForm.sfsh!='' && this.searchForm.sfsh!=undefined){\n            params['sfsh'] = this.searchForm.sfsh\n          }\n           if(this.searchForm.zuzhimingcheng!='' && this.searchForm.zuzhimingcheng!=undefined){\n    